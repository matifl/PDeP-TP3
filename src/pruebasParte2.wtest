import parte2.*

describe "Tests de Pau" {

	const pou = new Pou()
	const mochi = new Pou()
	const banana = new FreutasVerduras()

	test "Energia inicial de Pou" {
		assert.equals(30, pou.energiaInicial())
	}

	test "Tiene hambre Pou" {
		assert.that(pou.tieneHambre())
	}

	test "Mochi es vegetariana" {
		mochi.comer(banana)
	}

}

